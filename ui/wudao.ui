      <string notr="true">/* QTabWidget */
QTabWidget::pane {
    border: none;  /* 去除面板的边框 */
    background: white;  /* 设置面板背景为白色 */
}

/* Tab Bar */
QTabWidget::tab-bar {
    left: 5px;  /* 设置 tab-bar 与左边缘的间距 */
}

/* Tab 样式 */
QTabBar::tab {
    background: white;  /* 设置 tab 的背景为白色 */
    border: 2px solid #dcdcdc;  /* 设置边框颜色为浅灰色 */
    border-radius: 4px;  /* 设置圆角 */
    min-width: 80px;  /* 设置 tab 最小宽度 */
    padding: 10px;  /* 设置内边距 */
}

/* 选中的 tab 样式 */
QTabBar::tab:selected {
    background: #e0e0e0;  /* 选中时的背景色为浅灰色 */
    border-color: #ffffff;  /* 选中时边框的颜色 */
    font-weight: bold;  /* 设置选中时字体加粗 */
}

/* 未选中的 tab 样式 */
QTabBar::tab:!selected {
    background: #f0f0f0;  /* 未选中时背景颜色为非常浅的灰色 */
    margin-top: 5px;  /* 未选中时的间距 */
    border-color: #dcdcdc;  /* 未选中的边框颜色 */
}

/* Tab 下属界面 */
#tab, #tab_2, #tab_3, #tab_4 {
    background: white;  /* 设置四个 tab 界面的背景为白色 */
    border-radius: 6px;  /* 设置圆角 */
    padding: 10px;  /* 设置内边距 */
}
